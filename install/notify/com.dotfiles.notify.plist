<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN"
"http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.dotfiles.notify</string>

    <key>ProgramArguments</key>
    <array>
        <string>/Library/Application Support/Dotfiles/notify</string>
        <string>/Library/Application Support/Tarsnap/errors/</string>
    </array>

    <key>QueueDirectories</key>
    <array>
        <string>/Library/Application Support/Tarsnap/errors/</string>
    </array>

    <key>WorkingDirectory</key>
    <string>/Library/Application Support/Dotfiles</string>

    <key>StandardErrorPath</key>
    <string>/Library/Application Support/Dotfiles/notify-err.log</string>

    <key>StandardOutPath</key>
    <string>/Library/Application Support/Dotfiles/notify-out.log</string>

    <key>ProcessType</key>
    <string>Background</string>
</dict>
</plist>
